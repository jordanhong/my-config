# Launch matlab in terminal without opening matlab GUI
# matlabg is for usages that require figure displays
# matlab is for pure text-based computations. 
alias matlabg="/Applications/MATLAB_R2020a.app/bin/matlab -nodesktop -nosplash"
alias matlab="/Applications/MATLAB_R2020a.app/bin/matlab -nojvm -nodisplay -nosplash"
# weather
alias weather="curl wttr.in/Toronto"
alias skim="/Applications/Skim.app/Contents/MacOS/Skim"
# ls -a displays hidden files such as .bashrc, .zshrc, .vimrc
alias la="ls -a"
